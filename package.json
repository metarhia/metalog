{
  "name": "metalog",
  "version": "1.1.0",
  "author": "Timur Shemsedinov <timur.shemsedinov@gmail.com>",
  "description": "Meta Logger for Metarhia",
  "license": "MIT",
  "keywords": [
    "logger",
    "log",
    "metarhia",
    "impress",
    "jstp",
    "globalstorege"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/metarhia/metalog"
  },
  "main": "metalog.js",
  "readmeFilename": "README.md",
  "files": [],
  "scripts": {
    "test": "npm run -s lint && metatests test/",
    "lint": "eslint . && prettier -c \"**/*.js\" \"**/*.json\" \"**/*.md\" \".*rc\" \"**/*.yml\"",
    "fmt": "prettier --write \"**/*.js\" \"**/*.json\" \"**/*.md\" \".*rc\" \"**/*.yml\""
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "dependencies": {
    "@metarhia/common": "^2.1.0",
    "concolor": "^0.1.13",
    "metastreams": "^0.1.2"
  },
  "devDependencies": {
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.9.0",
    "eslint-plugin-prettier": "^3.1.2",
    "metatests": "^0.7.1",
    "prettier": "^1.19.1"
  }
}
